  <!--script src='https://api.tiles.mapbox.com/mapbox-gl-js/v0.44.1/mapbox-gl.js'></script>
<link href='https://api.tiles.mapbox.com/mapbox-gl-js/v0.44.1/mapbox-gl.css' rel='stylesheet' /-->
  <script src='https://api.tiles.mapbox.com/mapbox-gl-js/v0.49.0/mapbox-gl.js'></script>
  <link href='https://api.tiles.mapbox.com/mapbox-gl-js/v0.49.0/mapbox-gl.css' rel='stylesheet' />

  <div id="map" style="width: 800px; height: 800px">
  </div>
  <!--div>
    <%= message.cities[0].city_id %>
	<%= message.city_color %>
</div-->


  <script>
    mapboxgl.accessToken = 'pk.eyJ1IjoibWFkaHUzMTQiLCJhIjoiY2pjMXdsYW5hMDdmNzMybXdrM3o3MnRvdiJ9.DLm1UKSjtw6eWzzlAYBotQ';
    /*  var map = new mapboxgl.Map({
          container: 'map',
          style: 'mapbox://styles/madhu314/cjc8zh45k7fpi2spltb98qv5c',
          center: [77.118909062492, 28.6412210976328],
          zoom: 4,
          attributionControl: false,
          logoPosition: 'bottom-left'
      });
    */
    var map = new mapboxgl.Map({
      container: 'map',
      style: 'mapbox://styles/madhu314/cjc8zh45k7fpi2spltb98qv5c',//mapbox://styles/mapbox/streets-v9',
      center: [<%= message.lng %>,<%= message.lat %>],
      zoom: 3,
      attributionControl: false,
      logoPosition: 'bottom-left'
    });
    map.on('load', function () {
      map.addSource("country_map", {
        "type": "geojson",
        "data": { "type": "FeatureCollection", "features": [<%- message.national_cord %>] }
      })
      map.addSource("state_map", {
        "type": "geojson",
        "data": { "type": "FeatureCollection", "features":<%- message.state_cord %>}
      })
      map.addSource("city_map", {
        "type": "geojson",
        "data": {
          "type": "FeatureCollection", "features": [{
            "id": "<%= message.cities[0].city_id %>", "type": "Feature", "geometry": {
              "type": "MultiPolygon", "coordinates":<%= message.cities[0].city_cords %>
		  }, "properties": { "id": "<%= message.cities[0].city_id %>", "name": "Wazirpur" }
          }]
        }
      })

      map.addLayer({
        "id": "fill-layer",
        "type": "fill",
        "source": "country_map",
        "paint": {
          "fill-color": "#006400"
        },
        "filter": ["==", "$type", "Polygon"]
      });
      map.addLayer({
        "id": "stroke-layer",
        "type": "line",
        "source": "country_map",
        "paint": {
          "line-color": "#dddddd",
          "line-width": 1
        },
        "filter": ["==", "$type", "Polygon"]
      });

      map.addLayer({
        id: 'trees-heat',
        type: 'heatmap',
        source: 'city_map',
        maxzoom: 24,
        paint: {
          // increase weight as diameter breast height increases
          'heatmap-weight': {
            property: 'dbh',
            type: 'exponential',
            stops: [
              [1, 0],
              [62, 1]
            ]
          },
          // increase intensity as zoom level increases
          'heatmap-intensity': {
            stops: [
              [11, 1],
              [15, 3]
            ]
          },
          // assign color values be applied to points depending on their density
          'heatmap-color': [
            'interpolate',
            ['linear'],
            ['heatmap-density'],
            0, 'rgba(236,222,239,0)',
            0.2, '#<%= message.city_color %>',
            0.4, 'rgb(166,189,219)',
            0.6, 'rgb(103,169,207)',
            0.8, '#<%= message.city_color %>'
          ],
          // increase radius as zoom increases
          'heatmap-radius': {
            stops: [
              [11, 15],
              [15, 20]
            ]
          },
          // decrease opacity to transition into the circle layer
          'heatmap-opacity': {
            default: 1,
            stops: [
              [14, 1],
              [15, 0]
            ]
          },
        }
      });
      map.addLayer({
        id: 'trees-point',
        type: 'circle',
        source: 'city_map',
        minzoom: 32,
        paint: {
          // increase the radius of the circle as the zoom level and dbh value increases
          'circle-radius': {
            property: 'dbh',
            type: 'exponential',
            stops: [
              [{ zoom: 32, value: 1 }, 5],
              [{ zoom: 32, value: 62 }, 10],
              [{ zoom: 40, value: 1 }, 20],
              [{ zoom: 40, value: 62 }, 50],
            ]
          },
          'circle-color': {
            property: 'dbh',
            type: 'exponential',
            stops: [
              [0, 'rgba(236,222,239,0)'],
              [10, 'rgb(236,222,239)'],
              [20, 'rgb(208,209,230)'],
              [30, 'rgb(166,189,219)'],
              [40, 'rgb(103,169,207)'],
              [50, 'rgb(28,144,153)'],
              [60, 'rgb(1,108,89)']
            ]
          },
          'circle-stroke-color': 'white',
          'circle-stroke-width': 1,
          'circle-opacity': {
            stops: [
              [14, 0],
              [15, 1]
            ]
          }
        }
      });
      /*
      map.addLayer({
        id: 'city_map',
        source: 'city_map',
        "type": "heatmap",
          "minzoom": 3,
          "maxzoom": 9,
          "paint": {
              // Increase the heatmap weight based on frequency and property magnitude
              "heatmap-weight": [
                  "interpolate",
                  ["linear"],
                  ["get", "mag"],
                  0, 0,
                  6, 1
              ],
        "heatmap-radius": [40],
              // Increase the heatmap color weight weight by zoom level
              // heatmap-intensity is a multiplier on top of heatmap-weight
              "heatmap-intensity": [
                  "interpolate",
                  ["linear"],
                  ["zoom"],
                  0, 1,
                  9, 3
              ],
              // Color ramp for heatmap.  Domain is 0 (low) to 1 (high).
              // Begin color ramp at 0-stop with a 0-transparancy color
              // to create a blur-like effect.
              "heatmap-color": [
                  "interpolate",
                  ["linear"],
                  ["heatmap-density"],
                  1, "<%= message.city_color %>"
              ],
              // Adjust the heatmap radius by zoom level
              "heatmap-radius": [
                  "interpolate",
                  ["linear"],
                  ["zoom"],
                  0, 10,
                  20, 10
              ],
              // Transition from heatmap to circle layer by zoom level
              "heatmap-opacity": [
                  "interpolate",
                  ["linear"],
                  ["zoom"],
                  7, 1,
                  15, 0
              ],
          }
      });*/


      var coordinates = [[<%= message.cords %>]]
      var bounds = coordinates.reduce(function (bounds, coord) {
        return bounds.extend(coord);
      }, new mapboxgl.LngLatBounds(new mapboxgl.LngLat(coordinates[0][0], coordinates[0][1]), new mapboxgl.LngLat(coordinates[0][0], coordinates[0][1])));
      map.fitBounds(bounds, {
        padding: 10
      });
    });
  </script>

</body>

